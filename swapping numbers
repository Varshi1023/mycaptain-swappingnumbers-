#include <stdio.h>
int main()
{
   int a,b;
    printf("enter the value of a and value of b = \n");
    scanf("%d%d",&a,&b);
    printf("\n The entered values of a and b are %d and %d resp.",a,b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("\n The swapped numbers are a=%d and b=%d",a,b);
    return 0;
}
